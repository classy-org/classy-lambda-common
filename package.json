{
  "name": "classy-pay-core",
  "version": "0.0.5",
  "description": "Shared tools used in ClassyPay-related projects",
  "main": "lib/index.js",
  "scripts": {
    "test": "./node_modules/istanbul/lib/cli.js cover _mocha test/**/test*.js",
    "build": "babel --out-dir lib src --source-maps",
    "testbuild": "yarn run build && babel --copy-files --source-maps --out-dir testlib test",
    "prepublish": "yarn run clean; yarn run build",
    "clean": "rm -rfv lib testlib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brownjava/classy-pay-core.git"
  },
  "keywords": [
    "classy",
    "pay",
    "core"
  ],
  "author": "jeremy@brownjava.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brownjava/classy-pay-core/issues"
  },
  "homepage": "https://github.com/brownjava/classy-pay-core",
  "dependencies": {
    "bluebird": "^3.5.1",
    "bugsnag": "^2.1.3",
    "bunyan": "^1.8.12",
    "classy-api-client": "latest",
    "classy-node": "^2.5.7",
    "lodash": "^4.17.5",
    "nodecredstash": "^2.0.2",
    "regenerator-runtime": "^0.11.1",
    "request-promise": "^4.2.2",
    "source-map-support": "^0.5.4",
    "uuid": "^3.2.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.17.0",
    "eslint-config-google": "^0.9.1",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^5.1.1",
    "mocha-cli": "^1.0.1",
    "rewire": "^4.0.1",
    "should": "^13.2.1",
    "should-sinon": "^0.0.6",
    "sinon": "^5.0.7"
  },
  "files": [
    "lib"
  ]
}
